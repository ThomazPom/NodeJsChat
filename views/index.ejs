	<meta name="viewport" content="width=device-width, initial-scale=1">
	<script src="https://cdn.socket.io/socket.io-1.4.5.js"></script>
	<title>Virtual desktop</title>
</head>
<body>
	<div id="waiter">
		<img src="/images/loading.gif"/>
		<div class="meter">
			<span></span>
			<h3 style="color:white">Chargement en cours : <text>0%</text></h3>
		</div>
	</div>
	<div id="bg" ></div>
	<div id="content">
		<div id="divContent" class="divContent" style="display:none">
			<div class="divFrame" >
				<nav id="navbarFrame" class="navbar navbar-inverse">
					<a target="blank" data="#" class="title dropdown-toggle navbar-brand">
					</a><a onclick="$(this).parent().parent().parent().remove()" style="float:right" class="dropdown-toggle navbar-brand">Fermer :
					<span style="color:red" class="glyphicon glyphicon-remove" aria-hidden="true"></span></a>
				</nav>

			</div>
		</div>
	</div>

	<div id="wrapper">

		<!-- Sidebar -->
		<div id="sidebar-wrapper">
			<ul class="sidebar-nav">
				<li class="sidebar-brand">
					<a>
						Chat rooms
					</a>
				</li>
				<li >
					<a>
						Send messages as :
					</a>
				</li>
				<li class="sidebar-brand">
					<div class="container-fluid">
						<input type="text" id="nameInput" class="form-control" value="Anonymous"/>
					</div>
				</li>

					<li>
					<a>Ajouter une nouvelle chat room :</a>
				</li>
				<li class="sidebar-brand">
					<div class="container-fluid">
					<div class="row">
					<div class="col col-sm-9">
						<input type="text" id="newChannelInput" class="form-control" value=""/></div>
					
					<div class="col col-sm-1">
						<button class="btn btn-primary" id="newChannelButton" style="display: block;"><span class="glyphicon glyphicon-plus"> </span></button>
						</div>
						</div>
					</div>
				</li>
			</ul>
		</div>
		<nav class="navbar navbar-inverse navbar-fixed-bottom" id="navbarBot">
		<!--<ul class="nav navbar-nav navbar-right social">
			<li>
				<a class="navbar-brand appicon chatRoomLink" id="linkExplorer">Explorer les donn√©es<span style="color:cyan" class="glyphicon glyphicon-comment" aria-hidden="true"></span></a>
			</li>
		</ul>-->
